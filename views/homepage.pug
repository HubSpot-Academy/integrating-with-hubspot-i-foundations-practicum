doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table

        a(href="/update-cobj") Add/Update this table

        table
            thead
                tr
                    td Name
                    td Concert Type
                    td Composer
                    td Artist
                    td Concert Date
                    td Ticket Price
            each concerto in data
                tr
                    td #{concerto.properties.name}
                    td #{concerto.properties.type}
                    td #{concerto.properties.composer}
                    td #{concerto.properties.artist}
                    td #{concerto.properties.concert_date}
                    td $#{concerto.properties.ticket_price}