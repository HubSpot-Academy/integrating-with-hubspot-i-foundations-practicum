doctype html
html 
    head 
        title= title
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Cards
        a(href='/update-cobj') Add/Update this table
        if data.length
            table
                tr
                    th Card Name
                    th CMC
                    th Card Type
                each card in data
                    tr
                        td #{card.properties.card_name}
                        td #{card.properties.cmc}
                        td #{card.properties.card_type}
        else
            p No cards available.
