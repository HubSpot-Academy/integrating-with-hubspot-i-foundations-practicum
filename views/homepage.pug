doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1= `${title}`
        ul
            li
                a(href="/update-cobj") Add/Update this table
        table
            thead
            tr
                th Name
                th Breed
                th Birth Date
            tbody
            each dog in dogs
                tr
                td #{dog.properties.name}
                td #{dog.properties.breed}
                td #{dog.properties.birth_date}
